{
    "name": "UNet Attention",
    "arch": {
        "type": "UNetAttn",
        "args": {
            "max_bins": 1487
        }
    },
    "dataset": {
        "train": {
            "type": "FastMUSDB",
            "args": {
                "root": "../Demix/",
                "subsets": "train",
                "split": null,
                "seq_duration": 11.88,
                "samples_per_track": 32,
                "random": true,
                "random_track_mix": true
            }
        },
        "valid": {
            "type": "FastMUSDB",
            "args": {
                "root": "../Demix/",
                "subsets": "test",
                "split": null,
                "seq_duration": 11.88
            }
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 2e-4
        }
    },
    "loss": {
        "type": "CLoss",
        "args": {
            "mcoeff": 10,
            "n_fft": 4096,
            "hop_length": 1024
        }
    },
    "lr_scheduler": {
        "type": "ReduceLROnPlateau",
        "args": {
            "factor": 0.3,
            "patience": 80,
            "verbose": true
        }
    },
    "data_loader": {
        "train": {
            "batch_size": 16,
            "shuffle": true,
            "drop_last": true,
            "num_workers": 4,
            "prefetch_factor": 2,
            "pin_memory": true
        },
        "valid": {
            "batch_size": 4,
            "num_workers": 4,
            "prefetch_factor": 8,
            "pin_memory": true
        }
    },
    "trainer": {
        "epochs": 1000,
        "save_dir": "saved/Full_batch=16-4GPU-syncbatchnorm",
        "log_dir": "runs/Full_batch=16-4GPU-syncbatchnorm",
        "cum_steps": 1,
        "patience": 1000,
        "amp_enabled": false,
        "val_epoch_length": null,
        "validate_every": 1,
        "targets": [
            "vocals", "drums", "bass", "other"
        ],
        "n_fft": 4096,
        "hop_length": 1024,
        "extra_monitor": [
            "loss_f",
            "loss_t"
        ]
    },
    "MWF": {
        "residual_model": false,
        "softmask": false,
        "alpha": 1.0,
        "n_iter": 0
    }
}