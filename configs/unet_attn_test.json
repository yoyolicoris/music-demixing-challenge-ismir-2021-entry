{
    "name": "UNet Attention",
    "arch": {
        "type": "UNetAttn",
        "args": {
            "max_bins": 1487
        }
    },
    "dataset": {
        "train": {
            "type": "FastMUSDB",
            "args": {
                "root": "~/Datasets/musdb18hq/",
                "subsets": "train",
                "split": null,
                "seq_duration": 11.88,
                "samples_per_track": 32,
                "random": true,
                "random_track_mix": true
            }
        },
        "valid": {
            "type": "FastMUSDB",
            "args": {
                "root": "~/Datasets/musdb18hq/",
                "subsets": "train",
                "split": "valid",
                "seq_duration": 11.88
            }
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 2e-4
        }
    },
    "loss": {
        "type": "CLoss",
        "args": {
            "mcoeff": 10,
            "n_fft": 4096,
            "hop_length": 1024
        }
    },
    "lr_scheduler": {
        "type": "ReduceLROnPlateau",
        "args": {
            "factor": 0.3,
            "patience": 80,
            "verbose": true
        }
    },
    "data_loader": {
        "train": {
            "batch_size": 1,
            "shuffle": true,
            "drop_last": true,
            "num_workers": 2,
            "prefetch_factor": 2,
            "pin_memory": false
        },
        "valid": {
            "batch_size": 1,
            "num_workers": 2,
            "prefetch_factor": 2,
            "pin_memory": false
        }
    },
    "trainer": {
        "epochs": 1000,
        "save_dir": "saved/",
        "log_dir": "runs/",
        "cum_steps": 1,
        "patience": 1000,
        "amp_enabled": false,
        "val_epoch_length": null,
        "validate_every": 1,
        "targets": [
            "drums",
            "bass",
            "other",
            "vocals"
        ],
        "n_fft": 4096,
        "hop_length": 1024,
        "extra_monitor": [
            "loss_f",
            "loss_t"
        ]
    },
    "MWF": {
        "residual_model": false,
        "softmask": false,
        "alpha": 1.0,
        "n_iter": 0
    }
}